{
  "name": "project-corkscrew",
  "description": "extracting tmos config",
  "author": "",
  "version": "0.1.0",
  "license": "Apache-2.0",
  "homepage": "https://github.com/f5devcentral/project-corkscrew#readme",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/f5devcentral/project-corkscrew.git"
  },
  "scripts": {
    "lint": "tsc --noEmit && eslint 'src/**/*.{js,ts,tsx}' 'src/tests/**/*.{js,ts,tsx}'",
    "test": "nyc mocha -r ts-node/register \"tests/**/*.test.ts\"",
    "build-code-docs": "typedoc --out code_docs src",
    "build-package": "tsc"
  },
  "devDependencies": {
    "@types/assert": "^1.5.2",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.10.1",
    "@typescript-eslint/eslint-plugin": "^4.1.0",
    "@typescript-eslint/parser": "^4.1.0",
    "assert": "^2.0.0",
    "eslint": "^7.9.0",
    "mocha": "^8.1.3",
    "nyc": "^15.1.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  },
  "bugs": {
    "url": "https://github.com/f5devcentral/project-corkscrew/issues"
  },
  "nyc": {
    "check-coverage": false,
    "lines": 50,
    "functions": 50,
    "branches": 50,
    "statements": 50,
    "reporter": [
      "html",
      "text"
    ],
    "cache": false
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "extends": [
      "plugin:@typescript-eslint/recommended"
    ],
    "rules": {
      "no-unused-vars": "off"
    }
  },
  "mocha": {
    "timeout": 10000
  },
  "dependencies": {
    "@types/lodash": "^4.14.161",
    "lodash": "^4.17.20"
  }
}
